# OpenAI Configuration (.NET uses __ for nested properties: Section__Property)
# Required: Get your API key from https://platform.openai.com/api-keys
OpenAI__ApiKey=sk-your-api-key-here
OpenAI__EmbeddingModel=text-embedding-3-small
OpenAI__ChatModel=gpt-4o-mini

# Qdrant Vector Database
Qdrant__Host=http://localhost:6333
Qdrant__CollectionName=stackoverflow_chunks

# Redis Cache
# Caches embeddings and LLM responses for improved performance
Redis__Enabled=true
Redis__ConnectionString=localhost:6379
Redis__CacheTtlHours=24

# Data Ingestion
Ingestion__CsvPath=./data/stacksample.csv
Ingestion__MaxRows=10000
Ingestion__ChunkSize=500
Ingestion__ChunkOverlap=50

# Retrieval Configuration
DEFAULT_TOP_K=10
VECTOR_WEIGHT=0.5
KEYWORD_WEIGHT=0.5

# Logging
LOG_LEVEL=Information
