{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": ["FromLogContext"]
  },
  "Ingestion": {
    "CsvPath": "./data/stacksample.csv",
    "MaxRows": 10000,
    "ChunkSize": 500,
    "ChunkOverlap": 50
  },
  "OpenAI": {
    "ApiKey": "",
    "EmbeddingModel": "text-embedding-3-small",
    "ChatModel": "gpt-4o-mini"
  },
  "Comment": "API keys should be set via environment variables (OpenAI__ApiKey) - see .env.example",
  "Qdrant": {
    "Host": "http://localhost:6333",
    "CollectionName": "stackoverflow_chunks"
  },
  "AllowedHosts": "*"
}
